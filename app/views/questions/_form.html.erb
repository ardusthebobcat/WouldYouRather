<%= form_for @question, remote: true do |f| %>
  <%= f.label :answer_A %>
  <%= f.text_field :answer_A %>
  <%= f.label :picture_A %>
  <%= f.file_field :picture_A %>
<br />
  <%= f.label :answer_B %>
  <%= f.text_field :answer_B %>
  <%= f.label :picture_B %>
  <%= f.file_field :picture_B %>
<br />
<%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= f.submit %>
<% end %>
